{
  "presets": [
    [
      "@babel/env",
      {
        "bugfixes": true,
        "corejs": "3.22",
        "useBuiltIns": "usage"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true,
        "allowDeclareFields": true,
        "onlyRemoveTypeImports": true,
        "optimizeConstEnums": true
      }
    ]
  ]
}
